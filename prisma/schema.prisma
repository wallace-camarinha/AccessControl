generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  id         String   @id() @default(uuid())
  name       String
  email      String
  type       String?
  document   String?
  created_at DateTime @default(now())

  @@map("customers")
}

